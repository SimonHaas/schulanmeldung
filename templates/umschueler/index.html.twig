{% extends 'base.html.twig' %}

{% block title %}Umschueler index{% endblock %}

{% block body %}
    <h1>Umschueler index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Traeger</th>
                <th>TraegerSitz</th>
                <th>FoerdererNr</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for umschueler in umschuelers %}
            <tr>
                <td>{{ umschueler.id }}</td>
                <td>{{ umschueler.traeger }}</td>
                <td>{{ umschueler.traegerSitz }}</td>
                <td>{{ umschueler.foerdererNr }}</td>
                <td>
                    <a href="{{ path('umschueler_show', {'id': umschueler.id}) }}">show</a>
                    <a href="{{ path('umschueler_edit', {'id': umschueler.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('umschueler_new') }}">Create new</a>
{% endblock %}
