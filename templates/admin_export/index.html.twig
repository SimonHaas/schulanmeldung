{% extends 'base-admin.html.twig' %}

{% block title %}Hello AdminCreateImportController!{% endblock %}

{% block body %}
    <style>
        .example-wrapper {
            margin: 1em auto;
            max-width: 800px;
            width: 95%;
            font: 18px/1.5 sans-serif;
        }

        .example-wrapper code {
            background: #F5F5F5;
            padding: 2px 6px;
        }
    </style>

    <div class="example-wrapper">
        <a href="{{ path('admin') }}">Zur√ºck zur Verwaltung...</a>
        <div>
            {% if import_strings|length > 0 %}
                <pre>
                        {% for import_string in import_strings %}
                            {{ import_string }}
                        {% endfor %}
                </pre>
            {% else %}
                <p>Es gibt aktuell keine Registrierungen</p>
            {% endif %}
        </div>
    </div>
{% endblock %}
